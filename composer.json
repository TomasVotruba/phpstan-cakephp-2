{
    "name": "tomasvotruba/phpstan-cakephp2",
    "description": "An extension to help test CakePHP 2 projects with PHPStan",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Zhulnev",
            "email": "plbsid@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "phpstan/phpstan": "^1.9",
        "doctrine/inflector": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "PHPStanCakePHP2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPStanCakePHP2\\Test\\": "tests/"
        },
        "classmap": [
            "stubs",
            "tests/Feature/classes"
        ]
    },
    "scripts": {
        "phpstan": "phpstan",
        "phpunit": "phpunit",
        "test": [
            "@phpunit",
            "@phpstan"
        ]
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
